<?xml version="1.0" encoding="utf-8"?>
<odoo>

  <!-- View account.asset.asset tree -->
  <record id="view_account_asset_asset_tree_inherit" model="ir.ui.view">
    <field name="name">view.account.asset.asset.tree.inherit</field>
    <field name="model">account.asset.asset</field>
    <field name="inherit_id" ref="om_account_asset.view_account_asset_asset_purchase_tree"/>
    <field name="arch" type="xml">
      <xpath expr="//tree" position="attributes">
        <attribute name="js_class">asset_list</attribute>
      </xpath>
    </field>
  </record>

</odoo>
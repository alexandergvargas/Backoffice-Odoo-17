<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="hr_payroll.hr_salary_attachment_type_attachment" model="hr.salary.attachment.type">
        <field name="name">Movilidad</field>
        <field name="code">MOV</field>
        <field name="no_end_date" eval="True"/>
        <field name="country_id" ref="base.pe"/>
    </record>

    <record id="hr_payroll.hr_salary_attachment_type_assignment" model="hr.salary.attachment.type">
        <field name="name">Refrigerio</field>
        <field name="code">REF</field>
        <field name="country_id" ref="base.pe"/>
    </record>

    <record id="hr_payroll.hr_salary_attachment_type_child_support" model="hr.salary.attachment.type">
        <field name="name">Bono Productividad</field>
        <field name="code">BON_PRO</field>
        <field name="no_end_date" eval="False"/>
        <field name="country_id" ref="base.pe"/>
    </record>

</odoo>

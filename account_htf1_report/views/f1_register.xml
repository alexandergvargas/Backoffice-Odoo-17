<?xml version="1.0" encoding="utf-8"?>
<odoo>
	<data>
		<record model="ir.ui.view" id="view_f1_register_tree">
			<field name="name">f1.register.tree</field>
			<field name="model">f1.register</field>
			<field name="arch" type="xml">
				<tree>
					<field name="mayor"/>
					<field name="cuenta"/>
					<field name="nomenclatura"/>
					<field name="debe"/>
					<field name="haber"/>
					<field name="saldo_deudor"/>
					<field name="saldo_acreedor"/>
					<field name="activo"/>
					<field name="pasivo"/>
					<field name="perdinat"/>
					<field name="ganannat"/>
					<field name="perdifun"/>
					<field name="gananfun"/>
					<field name="rubro" optional="hide"/>
					<field name="group_balance" optional="hide"/>
					<field name="group_nature" optional="hide"/>
					<field name="group_function" optional="hide"/>
					<button name="view_detail" icon="fa-eye" type="object" 
							invisible="nomenclatura in ['UTILIDAD O PERDIDA','SUMAS']"/>
				</tree>
			</field>
		</record>
	</data>
</odoo>
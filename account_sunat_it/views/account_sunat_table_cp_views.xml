<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <!-- View account.sunat.table.cp View Tree -->
    <record id="view_account_sunat_table_cp_tree" model="ir.ui.view">
        <field name="name">view.account.sunat.table.cp.tree</field>
        <field name="model">account.sunat.table.cp</field>
        <field name="arch" type="xml">
            <tree editable="bottom" create="1" delete="1">
                <field name="name"/>
                <field name="code"/>
            </tree>
        </field>
    </record>




    <!-- Action account.sunat.table.cp -->
    <record id="action_account_sunat_table_cp" model="ir.actions.act_window">
        <field name="name">Cambios en el Patrimonio</field>
        <field name="type">ir.actions.act_window</field>
        <field name="res_model">account.sunat.table.cp</field>
        <field name="view_mode">tree</field>
        <field name="domain">[]</field>
        <field name="context">{}</field>
        <field name="help" type="html">
            <p class="oe_view_nocontent_create">
                Tabla para generar cambios en el patrimonio neto
            </p>
        </field>
    </record>

</odoo>

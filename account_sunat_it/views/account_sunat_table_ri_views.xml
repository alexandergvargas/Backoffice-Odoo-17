<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <!-- View account.sunat.table.ri View Tree -->
    <record id="view_account_sunat_table_ri_tree" model="ir.ui.view">
        <field name="name">view.account.sunat.table.ri.tree</field>
        <field name="model">account.sunat.table.ri</field>
        <field name="arch" type="xml">
            <tree editable="bottom" create="1" delete="1">
                <field name="name"/>
                <field name="code"/>
            </tree>
        </field>
    </record>


    <!-- Action account.sunat.table.ri -->
    <record id="action_account_sunat_table_ri" model="ir.actions.act_window">
        <field name="name">Resultados Integrales</field>
        <field name="type">ir.actions.act_window</field>
        <field name="res_model">account.sunat.table.ri</field>
        <field name="view_mode">tree</field>
        <field name="domain">[]</field>
        <field name="context">{}</field>
        <field name="help" type="html">
            <p class="oe_view_nocontent_create">
                Tabla para los Resultados Integrales
            </p>
        </field>
    </record>

</odoo>

<?xml version="1.0" encoding="utf-8"?>
<odoo>
	<record id="view_account_main_parameter_form_inherit_detrac" model="ir.ui.view">
		<field name="name">account.main.parameter.form.inherit.detrac</field>
		<field name="model">account.main.parameter</field>
		<field name="inherit_id" ref="account_base_it.view_account_main_parameter_form"/>
		<field name="arch" type="xml">
			<xpath expr="//notebook" position="inside">
				<page string="Detracciones" name='detrac'>
					<group>
						<group>
							<field name="auto_detrac"/>
						</group>
					</group>
				</page>
			</xpath>
		</field>
	</record>
</odoo>
<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="view_account_move_inherit_invisible_form_it" model="ir.ui.view">
        <field name="name">account.move.inherit.form.invisible.it</field>
		<field name="model">account.move</field>
		<field name="inherit_id" ref="account_fields_it.view_move_form_inherit"/>
        <field name="arch" type="xml">
            <xpath expr="//notebook//page[7]//group[1]//group[1]" position="attributes">
                <attribute name="invisible">move_type in ('out_invoice','out_receipt','in_receipt') or (register_sunat not in ('1','2') and move_type == 'entry')</attribute>
            </xpath>
        </field>
    </record>
</odoo>

<?xml version="1.0" ?>
<odoo>
    <data>
        <record model="ir.cron" id="sunat_exchange_rate_res_currency_cron">
            <field name="name">SUNAT - Exchange Rate</field>
            <field name="model_id" ref="base.model_res_currency"/>
            <field name="state">code</field>
            <field name="user_id" ref="base.user_root"/>
            <field name="interval_number">1</field>
            <field name="interval_type">days</field>
            <field name="numbercall">-1</field>
            <field name="doall">True</field>
            <field name="code">model._action_sunat_exchange_rate_pgm()</field>
            <field name="nextcall" eval="(DateTime.now() + timedelta(days=1)).strftime('%Y-%m-%d 10:00:00')"/>
        </record>
    </data>
</odoo>
